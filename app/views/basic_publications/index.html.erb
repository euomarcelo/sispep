<h1>Listing basic_publications</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Title</th>
      <th>Year</th>
      <th>Country</th>
      <th>Language</th>
      <th>Webpage</th>
      <th>English title</th>
      <th>Publication type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @basic_publications.each do |basic_publication| %>
      <tr>
        <td><%= basic_publication.profile_id %></td>
        <td><%= basic_publication.title %></td>
        <td><%= basic_publication.year %></td>
        <td><%= basic_publication.country %></td>
        <td><%= basic_publication.language %></td>
        <td><%= basic_publication.webpage %></td>
        <td><%= basic_publication.english_title %></td>
        <td><%= basic_publication.publication_type %></td>
        <td><%= link_to 'Show', basic_publication %></td>
        <td><%= link_to 'Edit', edit_basic_publication_path(basic_publication) %></td>
        <td><%= link_to 'Destroy', basic_publication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Basic publication', new_basic_publication_path %>
